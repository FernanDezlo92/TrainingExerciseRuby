<!DOCTYPE html>
<html>
    <%= render 'layouts/navbar/navbar' %>
    <h1><%= @organization.name %></h1>
        <table class="table table-hover">
        <thead>
            <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            </tr>
        </thead>
        <tbody>
            <% @seasons.each do |season| %>
                <tr>
                    <th scope="row"><%= season.id %></th>
                    <td><%= season.name %></td>
                </tr>
            <% end %>
        </tbody>
    </body>
</html>

