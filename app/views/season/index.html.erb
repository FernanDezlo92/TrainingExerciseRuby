<h1>List Season</h1>
<table class="table table-hover">
    <thead>
        <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Year</th>
        <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        <% @seasons.each do |season| %>
            <tr>
                <th scope="row"><%= season.id %></th>
                <td><%= season.name %></td>
                <td><%= season.season_year %></td>
                <td>
                    <%= link_to 'Show Season', organization_season_path(@organization.id, season.id), class: 'btn btn-sm btn-primary' %>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>
<%= link_to 'Back', organization_path(current_organization.id),  class: 'btn btn-sm btn-primary' %>
