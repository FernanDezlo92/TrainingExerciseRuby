<h1>List Round</h1>
<table class="table table-hover">
    <thead>
        <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Start Date</th>
        <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        <% @rounds.each do |round| %>
            <tr>
                <th scope="row"><%= round.id %></th>
                <td><%= round.name %></td>
                <td><%= round.start_date %></td>
                <td>
                    <%= link_to 'Show round', organization_season_round_path(current_organization.id, current_season.id, round.id), class: 'btn btn-sm btn-primary' %>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>
<%= link_to 'Back', organization_season_path(current_organization.id, current_season.id),  class: 'btn btn-sm btn-primary' %>

